    <!-- https://teamheka.github.io/meds-ontology#assignedSplit -->


    <owl:ObjectProperty rdf:about="https://teamheka.github.io/meds-ontology#assignedSplit">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Subject"/>
        <rdfs:range rdf:resource="https://teamheka.github.io/meds-ontology#SubjectSplit"/>
        <rdfs:comment>Assign a subject to a split (train, tuning, held_out).</rdfs:comment>
        <rdfs:label>assignedSplit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#hasCode -->


    <owl:ObjectProperty rdf:about="https://teamheka.github.io/meds-ontology#hasCode">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="https://teamheka.github.io/meds-ontology#Code"/>
        <rdfs:comment>Link from a Event to a Code metadata entry. If no Code metadata file exists, codeString on Event is used.</rdfs:comment>
        <rdfs:label>hasCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#hasSubject -->


    <owl:ObjectProperty rdf:about="https://teamheka.github.io/meds-ontology#hasSubject">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Event"/>
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="https://teamheka.github.io/meds-ontology#Subject"/>
        <rdfs:comment>Links a Event or LabelSample to its Subject (maps to subject_id).</rdfs:comment>
        <rdfs:label>hasSubject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#hasValueModality -->


    <owl:ObjectProperty rdf:about="https://teamheka.github.io/meds-ontology#hasValueModality">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="https://teamheka.github.io/meds-ontology#ValueModality"/>
        <rdfs:comment>Attach additional value modalities (image, waveform, etc.) to a Event.</rdfs:comment>
        <rdfs:label>hasValueModality</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#parentCode -->


    <owl:ObjectProperty rdf:about="https://teamheka.github.io/meds-ontology#parentCode">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Code"/>
        <rdfs:range rdf:resource="https://teamheka.github.io/meds-ontology#Code"/>
        <rdfs:comment>Ontological/aggregation parent relations between codes (maps to CodeMetadata.parent_codes).</rdfs:comment>
        <rdfs:label>parentCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://teamheka.github.io/meds-ontology#additionalValueModalityColumn -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#additionalValueModalityColumn">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column containing additional value modality (image, waveform, etc.). OPTIONAL. One triple per column. Matches MEDS: additional_value_modality_columns.</rdfs:comment>
        <rdfs:label>additionalValueModalityColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#booleanValue -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#booleanValue">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Boolean label value (if applicable).</rdfs:comment>
        <rdfs:label>booleanValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#categoricalValue -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#categoricalValue">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Categorical label encoded as a string.</rdfs:comment>
        <rdfs:label>categoricalValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#codeDescription -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#codeDescription">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Code"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Human readable description of the code.</rdfs:comment>
        <rdfs:label>codeDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#codeModifierColumn -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#codeModifierColumn">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column used to modify/augment code values. OPTIONAL. One triple per column. Matches MEDS: code_modifier_columns.</rdfs:comment>
        <rdfs:label>codeModifierColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#codeString -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#codeString">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://teamheka.github.io/meds-ontology#Code"/>
                    <rdf:Description rdf:about="https://teamheka.github.io/meds-ontology#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Canonical textual code; functional for Code. Also present on Event when no Code metadata exists.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#floatValue -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#floatValue">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Float label value (continuous target).</rdfs:comment>
        <rdfs:label>floatValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#integerValue -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#integerValue">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Integer label value (e.g., ordinal labels).</rdfs:comment>
        <rdfs:label>integerValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#medsVersion -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#medsVersion">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Version of the MEDS schema used to generate the dataset. REQUIRED. Matches MEDS: meds_version.</rdfs:comment>
        <rdfs:label>medsVersion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#numericValue -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#numericValue">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Numeric event value (optional in MEDS).</rdfs:comment>
        <rdfs:label>numericValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#otherExtensionColumn -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#otherExtensionColumn">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Other dataset-specific extension column not covered elsewhere. OPTIONAL. One triple per column. Matches MEDS: other_extension_columns.</rdfs:comment>
        <rdfs:label>otherExtensionColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#predictionTime -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#predictionTime">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Inclusive endpoint of the data usable for predicting the label (required for LabelSample).</rdfs:comment>
        <rdfs:label>predictionTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#rawSourceIdColumn -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#rawSourceIdColumn">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Raw source ID column before mapping to subject IDs. OPTIONAL. One triple per column. Matches MEDS: raw_source_id_columns.</rdfs:comment>
        <rdfs:label>rawSourceIdColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#siteIdColumn -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#siteIdColumn">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column representing a site identifier. OPTIONAL. One triple per column. Previously siteIdColumns.</rdfs:comment>
        <rdfs:label>siteIdColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#subjectId -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#subjectId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Subject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Unique identifier for a Subject; functional (= at most one value per Subject).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#subjectIdColumn -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#subjectIdColumn">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column used as a subject identifier. REQUIRED. One triple per column. Previously subjectIdColumns.</rdfs:comment>
        <rdfs:label>subjectIdColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#textValue -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#textValue">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Text event value (optional in MEDS).</rdfs:comment>
        <rdfs:label>textValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://teamheka.github.io/meds-ontology#time -->


    <owl:DatatypeProperty rdf:about="https://teamheka.github.io/meds-ontology#time">
        <rdfs:domain rdf:resource="https://teamheka.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp of the event. MEDS allows null for static events; modelled as 0..1 cardinality.</rdfs:comment>
        <rdfs:label>time</rdfs:label>
    </owl:DatatypeProperty>
    



    <!-- http://purl.org/dc/terms/LicenseDocument -->


    <owl:Class rdf:about="http://purl.org/dc/terms/LicenseDocument"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#Resource -->


    <owl:Class rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    


    <!-- http://www.w3.org/ns/dcat#Dataset -->


    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Dataset"/>
    


    <!-- http://www.w3.org/ns/dcat#Distribution -->


    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Distribution"/>
    


    <!-- http://www.w3.org/ns/prov#Activity -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Activity"/>
    


    <!-- http://www.w3.org/ns/prov#Entity -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Entity"/>
    


    <!-- https://teamheka.github.io/meds-ontology#Code -->


    <owl:Class rdf:about="https://teamheka.github.io/meds-ontology#Code">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
                <owl:allValuesFrom rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#codeString"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:hasKey rdf:parseType="Collection">
            <rdf:Description rdf:about="https://teamheka.github.io/meds-ontology#codeString"/>
        </owl:hasKey>
        <rdfs:comment>Metadata/vocabulary entry describing a event code (code string, description, parent codes, external links).</rdfs:comment>
        <rdfs:label>Code</rdfs:label>
    </owl:Class>
    


    <!-- https://teamheka.github.io/meds-ontology#DatasetMetadata -->


    <owl:Class rdf:about="https://teamheka.github.io/meds-ontology#DatasetMetadata">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/hasVersion"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/license"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/dc/terms/LicenseDocument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#distribution"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/prov#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/created"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/title"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#medsVersion"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>High-level provenance and conversion metadata for a MEDS dataset.</rdfs:comment>
        <rdfs:label>DatasetMetadata</rdfs:label>
    </owl:Class>
    


    <!-- https://teamheka.github.io/meds-ontology#Event -->


    <owl:Class rdf:about="https://teamheka.github.io/meds-ontology#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
                <owl:allValuesFrom rdf:resource="https://teamheka.github.io/meds-ontology#DatasetMetadata"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#hasCode"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#hasSubject"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#numericValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#textValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://teamheka.github.io/meds-ontology#time"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://teamheka.github.io/meds-ontology#LabelSample"/>
        <rdfs:comment>A single observation/row in MEDS: (subject, time?, code, value modalities...). Time is nullable for static events.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
